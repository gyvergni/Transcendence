#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules/detect-libc/bin/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules/detect-libc/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/node_modules:/home/pinkdonkeyjuice/42_projects/node_modules:/home/pinkdonkeyjuice/node_modules:/home/node_modules:/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules/detect-libc/bin/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules/detect-libc/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/node_modules:/home/pinkdonkeyjuice/42_projects/node_modules:/home/pinkdonkeyjuice/node_modules:/home/node_modules:/node_modules:/home/pinkdonkeyjuice/42_projects/transcendence/dev_front/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detect-libc/bin/detect-libc.js" "$@"
else
  exec node  "$basedir/../detect-libc/bin/detect-libc.js" "$@"
fi
